{
  "expo": {
    "name": "Uber Clone",
    "slug": "uber-clone",
    "description": "This is a simple uber clone to test my knowledge on react-native.",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/uber-icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/uber-splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
